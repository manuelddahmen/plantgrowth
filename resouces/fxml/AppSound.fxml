        <HBox layoutX="74.0" layoutY="180.0" prefHeight="100.0" prefWidth="526.0">
            <children>
                <Button mnemonicParsing="false" text="DO" id="DO"/>
                <Button mnemonicParsing="false" text="RE" id="RE"/>
                <Button mnemonicParsing="false" text="MI" id="MI"/>
                <Button mnemonicParsing="false" text="FA" id="FA"/>
