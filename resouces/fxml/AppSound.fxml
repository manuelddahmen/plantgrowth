<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.121"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="AppSound">
    <children>
        <RadioButton layoutX="40.0" layoutY="60.0" mnemonicParsing="false" text="Sin"/>
