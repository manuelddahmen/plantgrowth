                <Button mnemonicParsing="false" text="LA" id="LA"/>
                <Button mnemonicParsing="false" text="SI" id="SI"/>
                <Button mnemonicParsing="false" text="DO" id="DO2"/>
            </children>
        </HBox>
        <HBox id="C" layoutX="74.0" layoutY="150.0" prefHeight="100.0" prefWidth="526.0">
            <children>
                <Button mnemonicParsing="false" text="DO#" id="DOd"/>
                <Button mnemonicParsing="false" text="RE#" id="REd"/>
                <Button mnemonicParsing="false" text="--"/>
                <Button mnemonicParsing="false" text="FA#" id="FAd"/>
                <Button mnemonicParsing="false" text="SOL#" id="SOLd"/>
                <Button mnemonicParsing="false" text="LA#" id="LAd"/>
                <Button mnemonicParsing="false" text="--"/>
                <Button mnemonicParsing="false" text="DO#" id="DO2d"/>
            </children>
        </HBox>
    </children>
